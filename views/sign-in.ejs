<% extend('layout') %>

<div class="signin">
    <h1>Sign in</h1>
    <form action="/sign-in" method="POST">
        <label for="username">Username: </label>
        <input name="username" type="text" />
        <br>
        <label for="password">Password: </label>
        <input name="password" type="password" />
        <br>
        <button class="btn">Sign in</button>
    </form>

    <%= error %>
        

</div>

